# Supabase PostgreSQL Database Configuration (direct connection to database)
DATABASE_URL="postgresql://postgres:example_password@db.example.supabase.co:5432/example_db"

# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL="https://example.supabase.co"
NEXT_PUBLIC_SUPABASE_ANON_KEY="example_anon_key_value"
SUPABASE_SERVICE_ROLE_KEY="example_service_role_key_value"

# JWT Secret for authentication (using Supabase JWT secret)
JWT_SECRET="example_jwt_secret_value" # Replace with your own secret in production

# Individual Postgres credentials (for reference)
POSTGRES_HOST="db.example.supabase.co"
POSTGRES_DATABASE="example_db"
POSTGRES_USER="example_user"
POSTGRES_PASSWORD="example_password"
